# API URL
# Для разработки
REACT_APP_API_URL=http://localhost:5000/api/v1
# Для продакшена 
# REACT_APP_API_URL=https://lesnoy-dvorik-backend.vercel.app/api/v1

# Режим приложения
REACT_APP_ENVIRONMENT=development
# REACT_APP_ENVIRONMENT=production

# Настройки сборки
GENERATE_SOURCEMAP=true
# GENERATE_SOURCEMAP=false # для production

# Настройки порта
PORT=3000

# Stripe публичный ключ
REACT_APP_STRIPE_PUBLIC_KEY=pk_test_your_key

# Настройки аналитики
REACT_APP_ANALYTICS_ID=UA-XXXXXXXXX-X 